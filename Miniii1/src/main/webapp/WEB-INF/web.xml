<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserServlet</servlet-name>
  	<display-name>UserServlet</display-name>
  	<description></description>
  	<servlet-class>com.ltts.controller.UserServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletLog</servlet-name>
  	<display-name>ServletLog</display-name>
  	<description></description>
  	<servlet-class>com.ltts.controller.ServletLog</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserServlet</servlet-name>
  	<url-pattern>/UserServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletLog</servlet-name>
  	<url-pattern>/ServletLog</url-pattern>
  </servlet-mapping>
</web-app>
